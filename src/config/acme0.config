# "1" : real concentrations
Na = 0../
K = 100.
Cl = 100.
#Na = 145.
#K  = 4.
#Cl = 149.

# "2" : concentrations/100
#Na = 1.45
#K  = 0.04
#Cl = 1.49

# Channel densities for membrane groups
[membrane.axon]
Nav   = 120.
#HH-Na = 120.
#HH-K  =  36.
Kv    =  36.

[membrane.soma]
Na-Traub     = 100.
Ca-Traub     = 1.0
K(DR)-Traub  = 135.
K(AHP)-Traub = 0.8
K(C)-Traub   = 20.
K(A)-Traub   = 0.5
