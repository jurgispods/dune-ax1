[general]
outputDir = out-acme0
diagnosticsFile = diagnostics.dat
outputTimeInterval = 1

configName = default
useLogScaling = no
useMembrane = no
refineMembrane = no
mV_output = yes
gammaPot = 1.0
gammaCon = 1.0
# one of {step, squarePulse, trianglePulse, smoothStep}
initConc = step
adaptiveTimeStep = no

d_memb = 10.
xmax = 50.


# Ion concentrations for the solutions
[solution_in]
# "1" : real concentrations
Na = 0.
K = 100.
Cl = 100.
#Na = 12.
#K  = 155.
#Cl = 167.002

# "2" : concentrations/100
#Na = 0.12
#K  = 1.55
#Cl = 1.6700

